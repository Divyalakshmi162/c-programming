#include <stdio.h>

//Compiler version gcc  6.3.0

int main()
{
  /* c program to calculate average marks */
    char name [20];
  printf("Enter your name:\n");
  fgets(name,20, stdin);
  
  int num1;
  int num2;
  int num3;
  printf("Enter the marks of physics: \n");
  scanf("%d", & num1);
  printf("Enter the marks of chemistry: \n");
  scanf("%d", & num2);
  printf("Enter the marks of mathematics: \n ");
  scanf("%d", & num3);
 
  printf("Average marks: %d",(num1+num2+num3)/3);
  return 0;
}
