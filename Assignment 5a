#include <stdio.h>

//Compiler version gcc  6.3.0

int main() {
  int age;
  printf("Enter your age:");
  scanf("%d", &age);
  
  if(age>=60){;
    printf("Your pension is Rs 10,000");
  }else if(age>=50){;
    printf("Your pension is Rs 5000");
  }else{;
    printf("Your pension is Rs 0");
  };
  
  return 0;
}
