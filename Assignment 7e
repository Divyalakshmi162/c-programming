#include <stdio.h>

//Compiler version gcc  6.3.0

int main()
{
  int number;
  printf("Enter a number:\n");
  scanf("%d",&number);
  
  if(number>0){;
    printf("%d is positive",number);
  }else if(number<0){;
    printf("%d is negative",number);
  }else {
    printf(" you have entered the value zero");
  }
  return 0;
}
